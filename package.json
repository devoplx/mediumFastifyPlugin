{
    "name": "@devoplx/mediumplugin",
    "version": "1.2.1",
    "description": "A fastify medium api plugin.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "private": false,
    "scripts": {
        "dev": "npx nodemon",
        "setup:git-hooks": "git config core.hooksPath .git-hooks",
        "eslint": "npx eslint .",
        "prettier": "prettier --write .",
        "build": "tsup "
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/devoplx/mediumFastifyPlugin.git"
    },
    "keywords": [
        "fastify",
        "plugin",
        "production",
        "package"
    ],
    "author": "Rj Manhas / Devoplx",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/devoplx/ProdifyPlus-fastify/issues"
    },
    "homepage": "https://github.com/devoplx/ProdifyPlus-fastify#readme",
    "devDependencies": {
        "@appnest/readme": "^1.2.7",
        "@types/bunyan": "^1.8.8",
        "@types/depd": "^1.1.32",
        "@types/node": "^20.3.1",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "eslint": "^8.43.0",
        "nodemon": "^2.0.22",
        "prettier": "2.8.8",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@changesets/cli": "^2.26.1",
        "@devoplx/prodifyplus-fastify": "^1.2.1",
        "@fastify/compress": "^6.4.0",
        "@fastify/error": "^3.3.0",
        "@fastify/helmet": "^10.1.1",
        "axios": "^1.4.0",
        "bunyan": "^1.8.15",
        "cheerio": "^1.0.0-rc.12",
        "depd": "^2.0.0",
        "fastify": "^4.18.0",
        "fastify-plugin": "^4.5.0",
        "fastify-request-timing": "^2.0.2",
        "fastify-user-agent": "^1.0.1",
        "tsup": "^7.0.0"
    },
    "tsup": {
        "dts": true,
        "bundle": false,
        "clean": true,
        "treeshake": true,
        "target": "node16",
        "format": [
            "cjs",
            "esm"
        ],
        "entry": [
            "src/**/*.ts"
        ]
    }
}
